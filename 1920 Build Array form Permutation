class Solution {
public:
    vector<int> buildArray(vector<int>& nums) {
        
        int n=nums.size();

        vector<int>ans;

        for(int i=0;i<n;i++){

            int a=nums[i];

            int b=nums[a];
            
            ans.push_back(b);
        }
        return ans;
    }
};
